{
  "workbench.colorTheme": "Deepdark Material Theme | Full Black Version",
  "window.zoomLevel": 0,
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "breadcrumbs.enabled": true,
  "window.titleBarStyle": "custom",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.quoteStyle": "single",
  "html.autoClosingTags": true,
  "html.format.enable": true,
  "html.suggest.html5": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorWidth": 3,
  "editor.fontFamily": "'monospace', monospace,'Droid Sans Mono', 'Droid Sans Fallback'",
  "editor.minimap.showSlider": "always",
  "files.trimTrailingWhitespace": true,
  "workbench.editor.tabSizing": "shrink",
  "window.title": "${dirty}${activeEditorLong}${separator}${rootName}",
  "typescript.preferences.quoteStyle": "single",
  "prettier.singleQuote": true,
  "editor.wordWrapColumn": 80,
  "[csharp]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-vscode.csharp"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
